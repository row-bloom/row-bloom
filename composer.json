{
    "name": "elaborate-code/row-bloom",
    "description": "This is my package row-bloom",
    "keywords": [
        "elaborate-code",
        "row-bloom"
    ],
    "homepage": "https://github.com/elaborate-code/row-bloom",
    "license": "MIT",
    "authors": [
        {
            "name": "medilies",
            "email": "medilies.contact@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "chrome-php/chrome": "^1.9",
        "phpoffice/phpspreadsheet": "^1.28",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ElaborateCode\\RowBloom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElaborateCode\\RowBloom\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}