{
    "name": "row-bloom/row-bloom",
    "description": "Table rows to pdf cards",
    "keywords": [
        "pdf",
        "table",
        "generation",
        "rendering"
    ],
    "homepage": "https://github.com/row-bloom/row-bloom",
    "license": "MIT",
    "authors": [
        {
            "name": "medilies",
            "email": "medilies.contact@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "chrome-php/chrome": "^1.9",
        "illuminate/container": "*",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "symfony/var-dumper": "^6.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "RowBloom\\RowBloom\\": "src/",
            "RowBloom\\CssLength\\": "src/CssLength/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RowBloom\\ChromePhpRenderer\\": "src/_drivers/ChromePhpRenderer",
            "RowBloom\\RowBloom\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}